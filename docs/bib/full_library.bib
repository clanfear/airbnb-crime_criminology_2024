@article{regaladoParentsDisciplineYoung2004,
  title = {Parents' {{Discipline}} of {{Young Children}}: {{Results From}} the {{National Survey}} of {{Early Childhood Health}}},
  shorttitle = {Parents' {{Discipline}} of {{Young Children}}},
  author = {Regalado, Michael and Sareen, Harvinder and Inkelas, Moira and Wissow, Lawrence S. and Halfon, Neal},
