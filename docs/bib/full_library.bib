@article{rosenzweigAreThereIncreasing1994,
  title = {Are {{There Increasing Returns}} to the {{Intergenerational Production}} of {{Human Capital}}? {{Maternal Schooling}} and {{Child Intellectual Achievement}}},
  shorttitle = {Are {{There Increasing Returns}} to the {{Intergenerational Production}} of {{Human Capital}}?},
  author = {Rosenzweig, Mark R. and Wolpin, Kenneth I.},
  year = {1994},
  journal = {The Journal of Human Resources},
  volume = {29},
  number = {2},
  eprint = {146115},
  eprinttype = {jstor},
  pages = {670--693},
  issn = {0022-166X},
  doi = {10.2307/146115},
  urldate = {2018-07-16},
  abstract = {Information on ability and achievement test scores of sibling children, many of whom had mothers who continued their schooling between births, is used to test the hypothesis that maternal schooling augments the production of children's human capital, that there are increasing returns to human capital. Estimates from models that take into account heterogeneity in maternal endowments could not reject this hypothesis and suggest benefits to postponed childbearing. In particular, they suggest that postponement of the initiation of childbearing by two years among women who are tenth-graders would result in a 5 percent increase in their children's achievement test scores.}
}

@article{rosenzweigSistersSiblingsMothers1995,
  title = {Sisters, {{Siblings}}, and {{Mothers}}: {{The Effect}} of {{Teen-Age Childbearing}} on {{Birth Outcomes}} in a {{Dynamic Family Context}}},
  shorttitle = {Sisters, {{Siblings}}, and {{Mothers}}},
  author = {Rosenzweig, Mark R. and Wolpin, Kenneth I.},
  year = {1995},
  journal = {Econometrica},
  volume = {63},
  number = {2},
  eprint = {2951628},
  eprinttype = {jstor},
  pages = {303--326},
  issn = {0012-9682},
  doi = {10.2307/2951628},
  urldate = {2018-03-31},
  abstract = {A statistical model of dynamic intrafamily investment behavior incorporating endowment heterogeneity is estimated to evaluate alternative estimation procedures that have exploited family and kinship data. These procedures, which place alternative restrictions on the endowment structure and on behavior, include generalized least squares, instrumental-variables, fixed-effects based on the children of sisters, fixed-effects based on siblings, and sibling fixed-effects with instrumental variables. The framework is applied to data on birth outcomes, with focus on the effects of teen-age childbearing net of other maternal behavior. The empirical results imply that the least restrictive statistical formulation, consistent with dynamic behavior and heterogeneity among siblings, fits the data best. All of the estimation procedures that control for a family-specific endowment indicate, however, that the biological effect of having a birth at younger ages is to marginally increase birthweight and to increase fetal growth.}
}

