@article{squiresPrivilegedPlacesRace2005,
  title = {Privileged {{Places}}: {{Race}}, {{Uneven Development}} and the {{Geography}} of {{Opportunity}} in {{Urban America}}},
  shorttitle = {Privileged {{Places}}},
  author = {Squires, Gregory D. and Kubrin, Charis E.},
