@article{robinsSturdyChildhoodPredictors1978,
  title = {Sturdy Childhood Predictors of Adult Antisocial Behaviour: Replications from Longitudinal Studies},
  shorttitle = {Sturdy Childhood Predictors of Adult Antisocial Behaviour},
  author = {Robins, Lee N.},
  year = {1978},
