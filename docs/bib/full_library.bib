@article{branicGrowingPainsAppreciable2018,
  title = {Growing Pains or Appreciable Gains? {{Latent}} Classes of Neighborhood Change, and Consequences for Crime in {{Southern California}} Neighborhoods},
  shorttitle = {Growing Pains or Appreciable Gains?},
  author = {Branic, Nicholas and Hipp, John R.},
  year = {2018},
