@article{rowhani-rahbarTrendLoadedHandgun2022,
  title = {Trend in Loaded Handgun Carrying among Adult Handgun Owners in the {{United States}}, 2015-2019},
  author = {{Rowhani-Rahbar}, Ali and Gallagher, Amy and Azrael, Deborah and Miller, Matthew},
  year = {2022},
