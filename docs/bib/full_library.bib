@article{kreagerUrbanRevitalizationSeattle2011,
  title = {Urban {{Revitalization}} and {{Seattle Crime}}, 1982--2000},
  author = {Kreager, Derek A. and Lyons, Christopher J. and Hays, Zachary R.},
  year = {2011},
