@article{bollenBootstrappingGoodnessofFitMeasures1992,
  title = {Bootstrapping {{Goodness-of-Fit Measures}} in {{Structural Equation Models}}},
  author = {Bollen, Kenneth A. and Stine, Robert A.},
  year = {1992},
  month = nov,
  journal = {Sociological Methods \& Research},
