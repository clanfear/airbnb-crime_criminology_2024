@article{akmanMothersPostpartumPsychological2006,
  title = {Mothers' Postpartum Psychological Adjustment and Infantile Colic},
  author = {Akman, I. and Ku{\c s}{\c c}u, K. and {\"O}zdemir, N. and Yurdakul, Z. and Solakoglu, M. and Orhan, L. and Karabekiroglu, A. and {\"O}zek, E.},
  year = {2006},
  month = may,
  journal = {Archives of Disease in Childhood},
  volume = {91},
  number = {5},
  pages = {417--419},
  issn = {0003-9888, 1468-2044},
  doi = {10.1136/adc.2005.083790},
  urldate = {2018-03-31},
  abstract = {Background: Infantile colic is a common problem of early infancy. There is limited data on the relation between postpartum maternal psychological problems and colic. Aim:: To investigate whether infantile colic is associated with postpartum mood disorders or insecure adult attachment style of the mother. Methods: Seventy eight mothers and newborns were enrolled in this prospective, longitudinal study. Maternal depressive symptoms were screened with Edinburgh Postpartum Depression Score (EPDS) and maternal anxiety was assessed with State-Trait Anxiety Inventory (STAI). The Adult Attachment Scale was used to determine the attachment style of the mother. Infantile colic was defined according to Wessel criteria. Results: Infantile colic was present in 17 infants (21.7\%); 12.9\% of the mothers had an EPDS â©¾13. The mean EPDS of the mothers whose infants had infantile colic (10.2{\textpm}6.0) was significantly higher than that of the mothers of infants without colic (6.3{\textpm}4.0). Among infants with infantile colic, 62.5\% had mothers who had insecure attachment style, whereas only 31.1\% of mothers had insecure attachment when the infant did not have infantile colic. Conclusion: Postpartum maternal depressive symptoms and insecure attachment style are associated with infantile colic. Screening and early intervention of postpartum depression might promote the health of both the mother and infant.},
  copyright = {Copyright 2006 Archives of Disease in Childhood},
  langid = {english},
  pmid = {16452109},
  keywords = {attachment style,infantile colic,postpartum depression},
  file = {C:\Users\cclan\Zotero\storage\66MRIATT\417.html}
}

@article{allardReconsideringUrbanDisadvantaged2013,
