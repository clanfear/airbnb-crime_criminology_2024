@article{kalmussSubsequentChildbearingTeenage1994,
  title = {Subsequent {{Childbearing Among Teenage Mothers}}: {{The Determinants}} of a {{Closely Spaced Second Birth}}},
  shorttitle = {Subsequent {{Childbearing Among Teenage Mothers}}},
  author = {Kalmuss, Debra S. and Namerow, Pearila Brickner},
  year = {1994},
